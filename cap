[10:54 pm, 18/7/2024] Aravinthan: public class Main {
    public static void main(String[] args) {
        // Example usage of Netconf class
        Netconf netconf = new Netconf("192.168.1.100", 830, "admin", "password");
        netconf.connect();
        netconf.sendRPC("<get-config/>");
        netconf.disconnect();

        // Example usage of Restconf class
        Restconf restconf = new Restconf("https://api.example.com/restconf/", "admin", "password");
        restconf.sendGetRequest("data/resource");
        restconf.sendPostRequest("operations/modify", "{ \"key\": \"value\" }");
        restconf.sendPutRequest("data/resource/123", "{ \"name\": \"updated resource\" }");
        restconf.sendDeleteRequest("data/resource/123");
    }
}
